
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/forAdminLayout/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/forAdminLayout/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/forAdminLayout/css/nprogress.css" />
    <link rel="stylesheet" href="~/forAdminLayout/css/green.css" />
    <link rel="stylesheet" href="~/forAdminLayout/css/bootstrap-progressbar-3.3.4.min.css" />
    <link rel="stylesheet" href="~/forAdminLayout/css/jqvmap.min.css" />
    <link rel="stylesheet" href="~/forAdminLayout/css/daterangepicker.css" />
    <link rel="stylesheet" href="~/forAdminLayout/css/custom.min.css" />

</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a asp-area="" asp-controller="AdminDashboard" asp-action="Index" class="site_title"><i class="fa fa-paw"></i> <span>AdminDashboard</span></a>
                    </div>

                    <div class="clearfix"></div>

                    <!-- menu profile quick info -->
                    <div class="profile clearfix">
                        <div class="profile_pic">
                            @{
                                if (ViewData["CurrentUserAvatar"] == null)
                                    ViewData["CurrentUserAvatar"] = "\\storage\\avatars\\unnamed.jpg";
                            }
                            <img src=@ViewData["CurrentUserAvatar"] alt="..." class="img-circle profile_img">
                        </div>
                        <div class="profile_info">
                            <span>Welcome,</span>
                            <h2>
                                @ViewData["CurrentUserName"]
                            </h2>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->
                    <br />

                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">

                        <div class="menu_section">                           
                            <ul class="nav side-menu">
                                <li>
                                    <a asp-area="" asp-controller="AdminDashboard" asp-action="Index"><i class="fa fa-home"></i> Home </a>
                                </li>
                                <li>
                                    <a asp-area="" asp-controller="Account" asp-action="Index"><i class="fa fa-home"></i> Users </a>
                                </li>
                                <li>
                                    <a asp-area="" asp-controller="Sports" asp-action="Index"><i class="fa fa-home"></i> Sports </a>
                                </li>
                                <li>
                                    <a asp-area="" asp-controller="TrainingHalls" asp-action="Index"><i class="fa fa-home"></i> Halls </a>
                                </li>
                                <li>
                                    <a asp-area="" asp-controller="TrainingTypes" asp-action="Index"><i class="fa fa-home"></i> TrainingTypes </a>
                                </li>
                                <li>
                                    <a asp-area="" asp-controller="Times" asp-action="Index"><i class="fa fa-home"></i> Times </a>
                                </li>
                                <li>
                                    <a asp-area="" asp-controller="Trainings" asp-action="Index"><i class="fa fa-home"></i> Trainings </a>
                                </li>
                            </ul>
                        </div>                      

                    </div>
                    <!-- /sidebar menu -->
                    <!-- /menu footer buttons -->
                    <div class="sidebar-footer hidden-small">
                        <a data-toggle="tooltip" data-placement="top" title="Profile">
                            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                        </a>

                        <a data-toggle="tooltip" data-placement="top" title="Logout" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
                            <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                        </a>
                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>

            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <div class="nav toggle">
                        <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                    </div>
                    <nav class="nav navbar-nav" >                        
                    </nav>
                </div>
            </div>

            <div class="right_col" role="main">                
                    @RenderBody()
            </div>

            <footer>
                <div class="pull-right">
                    FitnessClub  - Make the impossible possible! 2021
                </div>
                <div class="clearfix"></div>
            </footer>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/forAdminLayout/js/jquery.min.js"></script>
    <script src="~/forAdminLayout/js/bootstrap.bundle.min.js"></script>
    <script src="~/forAdminLayout/js/fastclick.js"></script>
    <script src="~/forAdminLayout/js/nprogress.js"></script>
    <script src="~/forAdminLayout/js/Chart.min.js"></script>
    <script src="~/forAdminLayout/js/gauge.min.js"></script>
    <script src="~/forAdminLayout/js/bootstrap-progressbar.min.js"></script>
    <script src="~/forAdminLayout/js/icheck.min.js"></script>
    <script src="~/forAdminLayout/js/skycons.js"></script>
    <script src="~/forAdminLayout/js/jquery.flot.js"></script>
    <script src="~/forAdminLayout/js/jquery.flot.pie.js"></script>
    <script src="~/forAdminLayout/js/jquery.flot.time.js"></script>
    <script src="~/forAdminLayout/js/jquery.flot.stack.js"></script>
    <script src="~/forAdminLayout/js/jquery.flot.resize.js"></script>
    <script src="~/forAdminLayout/js/jquery.flot.orderBars.js"></script>
    <script src="~/forAdminLayout/js/jquery.flot.spline.min.js"></script>
    <script src="~/forAdminLayout/js/curvedLines.js"></script>
    <script src="~/forAdminLayout/js/date.js"></script>
    <script src="~/forAdminLayout/js/jquery.vmap.js"></script>
    <script src="~/forAdminLayout/js/jquery.vmap.sampledata.js"></script>
    <script src="~/forAdminLayout/js/moment.min.js"></script>
    <script src="~/forAdminLayout/js/daterangepicker.js"></script>
    <script src="~/forAdminLayout/js/custom.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
